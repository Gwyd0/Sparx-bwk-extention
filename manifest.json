{

  "manifest_version": 2,
  "name": "Sparx-Bwk",
  "description": "Helps you complete your sparx faster.",
  "version": "1.0.0",
  "content_scripts": [
	{
	"matches": ["<all_urls>"],
	"js": ["content.js"]
	}
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SparxBWk",
    "browser_style": true
  }
}
